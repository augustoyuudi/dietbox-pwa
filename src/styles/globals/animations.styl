@keyframes float
  0%
    transform translateY(5px)

  50%
    transform translateY(-5px)

  100%
    transform translateY(5px)

@keyframes tilt-up
  0%
    transform translateY(0px)

  50%
    transform translateY(-5px)

  100%
    transform translateY(0px)

@keyframes spin
  0%
    transform rotate(0deg)

  100%
    transform rotate(360deg)